<!DOCTYPE html>
<html>
<head>
  <title>@ViewBag.Title</title>
    
  <link rel="stylesheet" href="http://code.jquery.com/ui/1.10.3/themes/smoothness/jquery-ui.css" />
  <link rel='stylesheet' href='http://bootswatch.com/amelia/bootstrap.min.css' />
    <link href="http://getbootstrap.com/examples/jumbotron-narrow/jumbotron-narrow.css" rel="stylesheet" />
  <link href="@Url.Content("~/Content/css/Site2.css")" rel="stylesheet"/> 
</head>
<body>
    
    <div class="container">
        <div class='bs-component'>
          <div class="menu cf">
              @{ Html.RenderAction("Menu", "Nav"); }
          </div>

          <div class="jumbotron jumbotron_my">
            @RenderBody()
          </div>

          <div id="footer"></div>
        </div>
    </div>

    <script src="@Url.Content("~/Scripts/third-party/jquery-1.10.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/third-party/jquery-ui-1.10.3.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/third-party/knockout-3.0.0.js")" type="text/javascript"></script>
    <script src="http://bootswatch.com/bower_components/bootstrap/dist/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="http://bootswatch.com/assets/js/bootswatch.js" type="text/javascript"></script>
    <!--[if lt IE 8]>
      <script type="text/javascript" src="cdnjs.cloudflare.com/ajax/libs/json2/20110223/json2.js"></script>
    <![endif] -->    
    <script type="text/javascript" src="/Home/UserScripts" id="userScriptsPlaceHolder"></script>
</body>
</html>
